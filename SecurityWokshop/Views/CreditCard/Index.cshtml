@model IEnumerable<SecurityWokshop.Models.CreditCardModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Name
        </th>
        <th>
            Number
        </th>
        <th>
            Expiry Month
        </th>
        <th>
            Expiry Year
        </th>
        <th>
            CVV
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.Raw(item.Name)
        </td>
        <td>
            @Html.Raw(item.Number)
        </td>
        <td>
            @Html.Raw(item.ExpiryMonth)
        </td>
        <td>
            @Html.Raw(item.ExpiryYear)
        </td>
        <td>
            @Html.Raw(item.Cvv)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
